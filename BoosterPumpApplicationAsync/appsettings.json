{
  "SerialPort": {
    "PortName": "COM4",
    "BaudRate": 115300,
    "Timeout": 20000
  },
  "Database": {
    "SubDirectory": "Dropbox\\_FlowMeasurement",
    "FilePrefix": "FlowControllerX",
    "Headline": "Timestamp;Second of day;Manifold-P;NW-F;SE-F;Sys-P;Speed;DSpeed;Bar1-P;Bar2-P;Bar1-T;Bar2-T;Manifold-T;NW-T;SE-T;Sys-T;Com-Grad;Com-Intc",
    "SeparatorCharacter": "\t"
  },
  "Measurement": {
    // time in millie seconds.
    "RoundTripTime": 200,
    "ManifoldPressureCorrection": -19.50,
    "FlowNorthWestCorrection": 0.72,
    "FlowSouthEastCorrection": 0.72,
    "SystemPressureCorrection": 7.0
  },
  "Controller": {
    "Enabled": true,
    "MinSpeedPct": 0.01,
    "EastGradient": 1,
    "WestGradient": 1,
    "CommonGradient": 0.2,
    "CommonIntercept": 0,
    "AverageSize": 15
  },
  "Alarm": {
    // Pressure in psi
    "MinSystemPressure": 2000
  }
}
