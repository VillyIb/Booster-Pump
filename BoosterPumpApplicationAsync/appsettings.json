{
  "SerialPort": {
    "PortName": "COM4",
    "BaudRate": 115300 ,
    "Timeout":  200
  },
  "Database": {
    "SubDirectory": "Dropbox\\_FlowMeasurement",
    "FilePrefix": "FlowControllerX",
    // time in seconds
    "FlushInterval" :  60 
  },
  "Measurement": {
    // time in millie seconds.
    "RoundTripTime": 200,
    "ManifoldPressureCorrection": -19.50,
    "FlowNorthWestCorrection": 0.72,
    "FlowSouthEastCorrection": 0.72,
    "SystemPressureCorrection": 7.0
  },
  "Controller": {
    "Enabled": true,
    "MinSpeedPct": 1,
    "EastGradient": 1,
    "EastIntercept": 0,
    "WestGradient": 1,
    "WestIntercept": 0,
    "CommonGradient": 1,
    "CommonIntercept": 0,
    "AverageSize":  25
  },
  "Alarm": {
    // Pressure in psi
    "MinSystemPressure": 2000
  } 
}
